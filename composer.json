{
  "name": "naoray/nova-json",
  "description": "Nova json field to spread a json column throughout multiple fields.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "laravel",
    "nova"
  ],
  "authors": [
    {
      "name": "Krishan Koenig",
      "email": "krishan.koenig@googlemail.com"
    }
  ],
  "require": {
    "php": ">=7.4||^8.0.2",
    "illuminate/http": "^8.0||^9.0",
    "illuminate/support": "^8.0||^9.0",
    "laravel/nova": "^3.0||^4.0"
  },
  "autoload": {
    "psr-4": {
      "Naoray\\NovaJson\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Naoray\\NovaJson\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0||^7.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
