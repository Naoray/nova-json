{
  "name": "naoray/nova-json",
  "description": "Nova json field to spread a json column throughout multiple fields.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "laravel",
    "nova"
  ],
  "authors": [
    {
      "name": "Artem Stepanenko",
      "email": "stpnnk.artem@gmail.com"
    },
    {
      "name": "Krishan Koenig",
      "email": "krishan.koenig@googlemail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/http": "^9.0",
    "illuminate/support": "^9.0",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Naoray\\NovaJson\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Naoray\\NovaJson\\Tests\\": "tests"
    }
  },
