{
    "name": "naoray/nova-json",
    "description": "Nova json field to spread a json column throughout multiple fields.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "Krishan Koenig",
            "email": "krishan.koenig@googlemail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/http": "^9.0||^10.0||^11.0",
        "illuminate/support": "^9.0||^10.0||^11.0",
        "laravel/nova": "^3.0||^4.0",
        "cakephp/chronos": "^2.0||^3.0"
    },
    "autoload": {
        "psr-4": {
            "Naoray\\NovaJson\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naoray\\NovaJson\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0||^10.5",
        "orchestra/testbench": "^7.0||^8.0||^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
